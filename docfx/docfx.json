{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "EssSharp/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": "../src/"
        }
      ],
      "dest": "api/EssSharp",
      "filter": "filter.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    },
    {
      "src": [
        {
          "files": [
            "EssSharp.Abstractions/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": "../src/"
        }
      ],
      "dest": "api/EssSharp.Abstractions",
      "filter": "filter.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appFooter": "Copyright Â© 2022-2023 Applied OLAP",
      "_disableBreadcrumb": true,
      "_enableSearch": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/EssSharp"
    ],
    "postProcessors": ["EssSharpPostProcessor"],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}